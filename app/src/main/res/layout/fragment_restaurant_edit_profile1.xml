<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.fragment.homeCycle.restaurant.RestaurantEditProfileFragment1">


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/restaurant_edit_profile_fragment1_civ_image"
        android:layout_width="@dimen/dim_120dp"
        android:layout_height="@dimen/dim_120dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="@dimen/dim_16dp"
        tools:src="@drawable/kfc"/>

    <EditText
        android:id="@+id/restaurant_edit_profile_fragment1_et_name"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_user"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_24dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment1_civ_image"
        tools:text="دجاج كنتاكي"/>

    <EditText
        android:id="@+id/restaurant_edit_profile_fragment1_et_email"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_email"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment1_et_name"
        tools:text="kurishop@gmail.com"/>

    <RelativeLayout
        android:id="@+id/restaurant_edit_profile_fragment1_rl_city_spinner"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        android:padding="@dimen/dim_12dp"
        android:background="@drawable/shape_et_edit"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment1_et_email">

        <ImageView
            android:id="@+id/restaurant_edit_profile_fragment1_iv_city_sp_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_home"
            android:tint="@color/blue"
            android:layout_centerVertical="true"/>

        <Spinner
            android:id="@+id/restaurant_edit_profile_fragment1_sp_city_spinner"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_toEndOf="@id/restaurant_edit_profile_fragment1_iv_city_sp_icon"
            android:layout_toStartOf="@id/restaurant_edit_profile_fragment1_iv_city_sp_arrow"
            android:background="@null"
            android:padding="@dimen/dim_4dp"/>

        <ImageView
            android:id="@+id/restaurant_edit_profile_fragment1_iv_city_sp_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_down_arrow"
            android:layout_centerVertical="true"
            android:tint="@color/blue"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="@dimen/dim_4dp"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/restaurant_edit_profile_fragment1_rl_region_spinner"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        android:padding="@dimen/dim_12dp"
        android:background="@drawable/shape_et_edit"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment1_rl_city_spinner">

        <ImageView
            android:id="@+id/restaurant_edit_profile_fragment1_iv_region_sp_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_home"
            android:tint="@color/blue"
            android:layout_centerVertical="true"/>

        <Spinner
            android:id="@+id/restaurant_edit_profile_fragment1_sp_region_spinner"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_toEndOf="@id/restaurant_edit_profile_fragment1_iv_region_sp_icon"
            android:layout_toStartOf="@id/restaurant_edit_profile_fragment1_iv_region_sp_arrow"
            android:background="@null"
            android:padding="@dimen/dim_4dp"/>

        <ImageView
            android:id="@+id/restaurant_edit_profile_fragment1_iv_region_sp_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_down_arrow"
            android:layout_centerVertical="true"
            android:tint="@color/blue"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="@dimen/dim_4dp"/>

    </RelativeLayout>

    <EditText
        android:id="@+id/restaurant_edit_profile_fragment1_et_min_order_cost"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_money"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment1_rl_region_spinner"
        tools:text="10"/>

    <Button
        android:id="@+id/restaurant_edit_profile_fragment1_btn_continue"
        android:layout_width="0dp"
        android:layout_height="@dimen/dim_50dp"
        android:background="@drawable/shape_button"
        android:text="@string/continuee"
        android:textColor="@color/white"
        android:layout_marginBottom="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_12dp"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment1_et_min_order_cost"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>







</androidx.constraintlayout.widget.ConstraintLayout>