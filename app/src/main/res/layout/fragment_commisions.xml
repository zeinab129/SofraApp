<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="@dimen/dim_12dp"
    android:paddingStart="@dimen/dim_12dp"
    android:paddingEnd="@dimen/dim_12dp"
    tools:context=".view.fragment.homeCycle.restaurant.CommisionsFragment">

   <ImageView
       android:id="@+id/commisions_fragment_iv_image"
       android:layout_width="@dimen/dim_150dp"
       android:layout_height="@dimen/dim_300dp"
       android:src="@drawable/commisions"
       app:layout_constraintBottom_toBottomOf="parent"
       app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/commisions_fragment_tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/commisions"
        android:textSize="@dimen/dim_24sp"
        android:textStyle="bold"
        android:textColor="@color/blue"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/commisions_fragment_tv_details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textAlignment="center"
        android:textColor="@color/blue"
        android:textSize="@dimen/dim_20sp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="emwome foiwnowf niuw efwhif wnejnw ejn wicown ocnelnc  ndlnvlsdn;heuiwvls dnvskvviusjk kbvksekvbsklv vslbvjsbjhv"/>


    <TextView
        android:id="@+id/commisions_fragment_tv_restaurant_sales_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/restaurant_sales"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_details"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/commisions_fragment_tv_restaurant_sales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_details"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_restaurant_sales_title"
        tools:text="2000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/riel"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_details"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_restaurant_sales" />

    <TextView
        android:id="@+id/commisions_fragment_tv_app_commissions_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/application_commissions"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_restaurant_sales_title"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/commisions_fragment_tv_app_commissions"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_restaurant_sales_title"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_app_commissions_title"
        tools:text="2000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/riel"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_restaurant_sales_title"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_app_commissions" />



    <TextView
        android:id="@+id/commisions_fragment_tv_paid_off_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/what_was_paid_off"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_app_commissions_title"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/commisions_fragment_tv_paid_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_app_commissions_title"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_paid_off_title"
        tools:text="2000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/riel"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_app_commissions_title"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_paid_off" />


    <TextView
        android:id="@+id/commisions_fragment_tv_remaining_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/remaining"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_paid_off_title"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/commisions_fragment_tv_remaining"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_paid_off_title"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_remaining_title"
        tools:text="2000"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/riel"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_paid_off_title"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_remaining" />

    <TextView
        android:id="@+id/commisions_fragment_tv_raghy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/raghy"
        app:layout_constraintTop_toTopOf="@id/commisions_fragment_iv_image"
        app:layout_constraintBottom_toBottomOf="@id/commisions_fragment_iv_image"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_iv_image" />

    <TextView
        android:id="@+id/commisions_fragment_tv_raghy_account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="564894564"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toTopOf="@id/commisions_fragment_iv_image"
        app:layout_constraintBottom_toBottomOf="@id/commisions_fragment_iv_image"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_raghy" />

    <TextView
        android:id="@+id/commisions_fragment_tv_ahly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_4dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/ahly"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_raghy"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_iv_image" />

    <TextView
        android:id="@+id/commisions_fragment_tv_ahly_account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_4dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="4564564564"
        android:layout_marginStart="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_raghy"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_tv_ahly"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_8dp"
        android:textSize="@dimen/dim_16sp"
        android:textColor="@color/blue"
        android:text="@string/sofra"
        app:layout_constraintTop_toBottomOf="@id/commisions_fragment_tv_ahly"
        app:layout_constraintStart_toEndOf="@id/commisions_fragment_iv_image"/>








</androidx.constraintlayout.widget.ConstraintLayout>