<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.fragment.homeCycle.restaurant.RestaurantEditProfileFragment2">


    <EditText
        android:id="@+id/restaurant_edit_profile_fragment2_et_delivery_cost"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_money"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_32dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="20"/>

    <EditText
        android:id="@+id/restaurant_edit_profile_fragment2_et_delivery_time"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_clock"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment2_et_delivery_cost"
        tools:text="30"/>

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/restaurant_edit_profile_fragment2_sc_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        android:textColor="@color/blue"
        android:textSize="@dimen/dim_24sp"
        android:text="@string/status"
        android:checked="true"
        android:theme="@style/SCBSwitch"
        app:switchPadding="@dimen/dim_8dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment2_et_delivery_time"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/restaurant_edit_profile_fragment2_tv_connection_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/connection_data"
        android:textColor="@color/blue"
        android:textSize="@dimen/dim_20sp"
        android:textStyle="bold"
        android:layout_marginTop="@dimen/dim_16dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment2_sc_status"
        app:layout_constraintStart_toStartOf="parent"/>

    <EditText
        android:id="@+id/restaurant_edit_profile_fragment2_et_phone_number"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_phone"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment2_tv_connection_data"
        tools:text="01097163235"/>

    <EditText
        android:id="@+id/restaurant_edit_profile_fragment2_et_whatsapp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_60dp"
        android:background="@drawable/shape_et_edit"
        android:drawableStart="@drawable/ic_whatsapp"
        android:drawableTint="@color/blue"
        android:textColor="@color/blue"
        android:drawablePadding="@dimen/dim_8dp"
        android:padding="@dimen/dim_12dp"
        android:layout_marginTop="@dimen/dim_12dp"
        android:layout_marginStart="@dimen/dim_12dp"
        android:layout_marginEnd="@dimen/dim_12dp"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment2_et_phone_number"
        tools:text="01097163235"/>

    <Button
        android:id="@+id/restaurant_edit_profile_fragment2_btn_edit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_button"
        android:text="@string/edit"
        android:textColor="@color/white"
        app:layout_constraintWidth_percent="0.5"
        app:layout_constraintTop_toBottomOf="@id/restaurant_edit_profile_fragment2_et_whatsapp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>




</androidx.constraintlayout.widget.ConstraintLayout>